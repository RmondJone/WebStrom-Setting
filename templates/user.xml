<templateSet group="user">
  <template name="c" value="注释: $a$&#10;* 时间: $b$ $c$&#10;* @author 郭翰林" description="函数注释" toReformat="false" toShortenFQNames="true">
    <variable name="a" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="b" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="c" expression="time()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="jt" value="()=&gt;" description="箭头函数" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="swi" value="switch ($a$) {&#10;  case $b$:&#10;    break&#10;  default:&#10;    break&#10;}" description="switch()" toReformat="false" toShortenFQNames="true">
    <variable name="a" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="b" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="initModel" value="import {plainToClass} from &quot;class-transformer&quot;;&#10;import {XModel} from &quot;../../../dva/model&quot;;&#10;&#10;&#10;interface $a$ extends XModel {&#10;    state: {}&#10;    stateTransformer: (state) =&gt; any,&#10;}&#10;&#10;const model: $a$ = {&#10;    namespace: '',&#10;    state: {&#10;    },&#10;    stateTransformer: (state) =&gt; {&#10;        return state;&#10;    },&#10;    effects: {},&#10;    subscriptions: {},&#10;    reducers: {}&#10;}&#10;&#10;export default model&#10;" description="initModel" toReformat="false" toShortenFQNames="true">
    <variable name="a" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="for" value="for (let i = 0; i &lt; $a$; i++) {&#10;&#10;}" description="for循环" toReformat="false" toShortenFQNames="true">
    <variable name="a" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>